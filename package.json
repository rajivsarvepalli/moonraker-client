{
  "private": false,
  "displayName": "Typescript Moonraker Client",
  "name": "moonraker-client",
  "version": "1.0.1",
  "description": "A typescript Moonraker (the Klipper Web API) client",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/rajivsarvepalli/moonraker-client.git"
  },
  "bugs": {
    "url": "https://github.com/rajivsarvepalli/moonraker-client/issues"
  },
  "engines": {
    "node": "^18.17.0 || ^20.9.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/**.ts --max-warnings=0",
    "watch": "npm run build && npm link && nodemon",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "keywords": [
    "klipper",
    "moonraker",
    "typescript"
  ],
  "dependencies": {
    "axios": "^1.6.7",
    "axios-retry": "^4.0.0",
    "url-join": "^5.0.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.20",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.45.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
